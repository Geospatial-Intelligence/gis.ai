# Domain and ACME
DOMAIN=gis.ai
ACME_EMAIL=your-email@example.com

# Docker registry
DOCKER_REGISTRY=registry.example.com/gisai
DOCKER_REGISTRY_USER=your-user
DOCKER_REGISTRY_PASSWORD=your-password

# Deploy target
DEPLOY_USER=deploy
FIXED_IP_HOST=203.0.113.10

# Postgres
POSTGRES_DB=gisai
POSTGRES_USER=gisai
POSTGRES_PASSWORD=change-me
